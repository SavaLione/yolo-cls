#ifndef CONFIG_H
#define CONFIG_H

/* Project version */

/**
 * @brief Defines the full project version string.
*/
#define PROJECT_VERSION "${PROJECT_VERSION}"

/**
 * @brief Defines the project major version.
*/
#define PROJECT_VERSION_MAJOR "${PROJECT_VERSION_MAJOR}"

/**
 * @brief Defines the project minor version.
*/
#define PROJECT_VERSION_MINOR "${PROJECT_VERSION_MINOR}"

/**
 * @brief Defines the project path version.
*/
#define PROJECT_VERSION_PATCH "${PROJECT_VERSION_PATCH}"

/* Project description */

/**
 * @brief Defines the project homepage url.
*/
#define PROJECT_HOMEPAGE_URL "${PROJECT_HOMEPAGE_URL}"

/**
 * @brief Defines the project author.
*/
#define PROJECT_AUTHOR "${PROJECT_AUTHOR}"

/**
 * @brief Defines the project license.
*/
#define PROJECT_LICENSE "${PROJECT_LICENSE}"

/* Project settings */

/**
 * @brief Defines a macro whether to use Nvidia CUDA or not.
*/
#cmakedefine YOLOCLS_USE_CUDA

#endif // CONFIG_H